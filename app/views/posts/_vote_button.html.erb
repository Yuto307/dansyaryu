<ul class='crud-menu-btn list-inline float-right'>
  <li class="list-inline-item">
    <% if current_user.favorite?(post) %>
      <%= render 'posts/vote_unanswer', post: post %>
    <% else %>
      <%= render 'posts/vote_trash', post: post %>
      <%= render 'posts/vote_untrash', post: post %>
    <% end %>
  </li>
</ul>